{
  "manifest_version": 2,

  "name": "wcmc.io URL shortener",
  "description": "This extension provides quick URL shortening using the wcmc.io service",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "clipboardWrite",
    "tabs",
    "http://wukumurl.unepwcmc-005.vm.brightbox.net/"
  ]
}
